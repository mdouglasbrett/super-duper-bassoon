{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "plugins": [
        "flowtype",
        "prettier",
        "mocha"
    ],
    "rules": {
        "prettier/prettier": "error",
        "mocha/no-exclusive-tests": "error",
        "comma-dangle": "off",
        "semi": "error",
        "implicit-arrow-linebreak": "off",
        "arrow-parens": "off",
        "function-paren-newline": "off",
        "max-len": "off"
    }
}